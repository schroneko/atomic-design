{
  "name": "storybook-adk",
  "version": "0.7.8",
  "description": "React Storybook Addon Development Kit",
  "main": "dist/index.js",
  "scripts": {
    "deploy": "node .scripts/deployer",
    "prepublish": "node .scripts/npm-prepublish.js",
    "postpublish": "node .scripts/npm-postpublish.js",
    "lint": "node .scripts/lint.js",
    "lintfix": "node .scripts/lint.js --fix",
    "testonly": "node .scripts/run_tests",
    "test": "npm run lint && npm run testonly",
    "test-watch": "node .scripts/run_tests --watch --watch-extensions jsx,json",
    "test-storybook": "storyshots",
    "status": "node .scripts/npm-status.js && node .scripts/lint.js -f json && node .scripts/run_tests -R min",
    "storybook": "start-storybook -p 9010 && npm run test-watch",
    "start": "start-storybook -p 9001",
    "watch": "nodemon -w ./src -e .js,.jsx -x \"npm run prepublish\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sm-react/storybook-adk"
  },
  "keywords": [
    "storybook",
    "react",
    "addon",
    "decorator",
    "customization",
    "boilerplate",
    "npm",
    "development",
    "addons"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sm-react/storybook-adk/issues"
  },
  "homepage": "https://github.com/sm-react/storybook-adk",
  "devDependencies": {
    "@storybook/addon-info": "^3.1.2",
    "@storybook/addon-knobs": "^3.1.2",
    "@storybook/addon-notes": "^3.1.2",
    "@storybook/addon-options": "^3.1.2",
    "@storybook/addon-storyshots": "^3.1.2",
    "@storybook/react": "^3.1.3",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.5.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "dateformat": "^2.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "git-url-parse": "^6.0.1",
    "jsdom": "^9.5.0",
    "material-ui": "^0.16.0",
    "mocha": "^3.2.0",
    "node-sass": "^3.10.0",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.3.0",
    "react-theme-provider": "^0.1.3",
    "sass-loader": "^4.0.2",
    "semver": "^5.3.0",
    "shelljs": "^0.7.3",
    "sinon": "^1.17.3",
    "storybook-addon-material-ui": "^0.7.9",
    "style-loader": "^0.13.1"
  },
  "peerDependencies": {
    "@storybook/react": "3.1.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "dependencies": {
    "@storybook/addons": "^3.1.2",
    "immutable": "^3.8.1",
    "podda": "^1.2.2",
    "prop-types": "^15.5.10",
    "react-komposer": "2"
  }
}
